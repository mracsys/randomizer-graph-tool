                        or
                        (((boulder_type('DEKU_COMPASS_BOULDER_1') == BOULDER_TYPE_RED_ICE) or (boulder_type('DEKU_COMPASS_BOULDER_2') == BOULDER_TYPE_RED_ICE)) and 
