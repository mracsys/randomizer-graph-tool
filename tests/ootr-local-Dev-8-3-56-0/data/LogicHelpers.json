{
    # Rules here define replacement "functions" or item aliases.
    # The alias itself must always be alphanumeric only--string items bypass the aliasing.
    # Rules should be simple python and can invoke other aliases or even the
    # at/here metarules.
    "Hookshot": "Progressive_Hookshot",
    "Longshot": "(Progressive_Hookshot, 2)",
    "Silver_Gauntlets": "(Progressive_Strength_Upgrade, 2)",
    "Golden_Gauntlets": "(Progressive_Strength_Upgrade, 3)",
    "Scarecrow": "Progressive_Hookshot and can_play(Scarecrow_Song)",
    "Distant_Scarecrow": "(Progressive_Hookshot, 2) and can_play(Scarecrow_Song)",
    # Items can be directly aliased, so all occurrences of that item name get replaced,
    # but make sure that inside the rule you use the string 'item' form
    # to avoid infinite recursion.
    "Goron_Tunic": "'Goron Tunic' or Buy_Goron_Tunic",
    "Zora_Tunic": "'Zora Tunic' or Buy_Zora_Tunic",

    # Refill aliases. If we start considering Bow etc refills we can enable these aliases
    # without editing all logic files.
    # "Bow": "'Bow'",
    # "Slingshot": "'Slingshot'",
    "Bombs": "Bomb_Bag",
    "Deku_Shield": "Buy_Deku_Shield or Deku_Shield_Drop",
    "Hylian_Shield": "Buy_Hylian_Shield",
    "Nuts": "Buy_Deku_Nut_5 or Buy_Deku_Nut_10 or Deku_Nut_Drop",
    "Sticks": "Buy_Deku_Stick_1 or Deku_Stick_Drop",
    "Bugs": "'Bugs' or Buy_Bottle_Bug",
    "Blue_Fire": "'Blue Fire' or Buy_Blue_Fire or (blue_fire_arrows and can_use(Blue_Fire_Arrows))",
    "Fish": "'Fish' or Buy_Fish",
    "Fairy": "'Fairy' or Buy_Fairys_Spirit",
    # "Big_Poe": "'Big Poe'",

    "can_refill_bombchus": "Buy_Bombchu_5 or Buy_Bombchu_10 or Buy_Bombchu_20 or Bombchu_Drop",
    "has_bombchus": "(free_bombchu_drops and found_bombchus) or (not free_bombchu_drops and can_refill_bombchus and Bomb_Bag)",
    "found_bombchus": "(free_bombchu_drops and (Bombchus or Bombchus_5 or Bombchus_10 or Bombchus_20)) or (not free_bombchu_drops and Bomb_Bag)",
    "has_explosives": "Bombs or (free_bombchu_drops and has_bombchus)",
    "has_both_explosives": "Bombs and (free_bombchu_drops and has_bombchus)",
    "is_child": "age == 'child'",
    "is_adult": "age == 'adult'",
    "is_starting_age": "age == starting_age",

    "can_blast_or_smash": "has_explosives or can_use(Megaton_Hammer)",
    "can_break_mud_wall": "can_blast_or_smash or (blue_fire_arrows and can_use(Blue_Fire_Arrows))",
    "can_child_attack": "is_child and (Slingshot or Boomerang or Sticks or Kokiri_Sword or has_explosives or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_child_damage": "is_child and (Slingshot or Sticks or Kokiri_Sword or has_explosives or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_cut_shrubs": "is_adult or Sticks or Kokiri_Sword or Boomerang or has_explosives or can_use(Megaton_Hammer)",
    "can_dive": "Progressive_Scale",
    "can_leave_forest": "open_forest != 'closed' or is_adult or 'Defeat Queen Gohma'
        or glitch_itemless_forest_escape
        or (glitch_pokey_escape and Deku_Shield and (Kokiri_Sword or Sticks))
        or (glitch_wess_escape and (Kokiri_Sword or Sticks))",
    "can_plant_bugs": "is_child and Bugs",
    "can_ride_epona": "is_adult and Epona and (can_play_ocarina(Eponas_Song) or (can_hover and not shuffle_overworld_entrances))",
    "can_stun_deku": "is_adult or (Slingshot or Boomerang or Sticks or Kokiri_Sword or has_explosives or can_use(Dins_Fire) or Nuts or Deku_Shield or can_use(Megaton_Hammer))",
    "can_summon_gossip_fairy": "can_play(Zeldas_Lullaby) or can_play(Eponas_Song) or can_play(Song_of_Time) or can_play(Suns_Song)",
    "can_summon_gossip_fairy_without_suns": "can_play(Zeldas_Lullaby) or can_play(Eponas_Song) or can_play(Song_of_Time)",
    "can_take_damage": "damage_multiplier != 'ohko' or Fairy or can_use(Nayrus_Love)",
    "can_plant_bean": "plant_beans or (is_child and (Magic_Bean, 10))",
    "can_play(song)": "(Ocarina or can_oi)
        and song and (not shuffle_individual_ocarina_notes or has_all_notes_for_song(song))",
    "can_play_ocarina(song)": "Ocarina
        and song and (not shuffle_individual_ocarina_notes or has_all_notes_for_song(song))",
    "can_play_item(song)": "(Ocarina or can_oi_with_item)
        and song and (not shuffle_individual_ocarina_notes or has_all_notes_for_song(song))",


    "can_open_bomb_grotto": "can_blast_or_smash and (Stone_of_Agony or logic_grottos_without_agony)",
    "can_open_storm_grotto": "can_play(Song_of_Storms) and (Stone_of_Agony or logic_grottos_without_agony)",
    # The last case in a conditional still needs a check, to prevent failure from falling through
    # into what should be the 'else' case.
    "can_use_projectile": "has_explosives or
        (is_adult and (Bow or Hookshot or can_use(Boomerang) or can_use(Slingshot))) or
        (is_child and (Slingshot or Boomerang))",
    "has_projectile(for_age)": "has_explosives
            or (for_age == child and (Slingshot or Boomerang))
            or (for_age == adult and (Bow or Hookshot or (can_equip_swap_adult and (Slingshot or Boomerang))))
            or (for_age == both and (Slingshot or Boomerang) and (Bow or Hookshot or (can_equip_swap_adult and (Slingshot or Boomerang))))
            or (for_age == either and (Slingshot or Boomerang or Bow or Hookshot))",
    "can_bonk": "deadly_bonks != 'ohko' or Fairy or can_use(Nayrus_Love)",
    "can_break_crate": "can_bonk or can_blast_or_smash",
    "can_break_heated_crate": "deadly_bonks != 'ohko' or (Fairy and (can_use(Goron_Tunic) or damage_multiplier != 'ohko')) or can_use(Nayrus_Love) or can_blast_or_smash",
    "can_break_lower_beehive": "can_use(Boomerang) or can_use(Hookshot) or Bombs or (logic_beehives_bombchus and has_bombchus)",
    "can_break_upper_beehive": "can_use(Boomerang) or can_use(Hookshot) or (logic_beehives_bombchus and has_bombchus)",
    "can_mow_the_lawn": "is_adult or (is_child and (Sticks or Kokiri_Sword or Progressive_Strength_Upgrade or Boomerang or has_explosives))",

    # can_use and helpers
    # The parser reduces this to smallest form based on item category.
    # Note that can_use(item) is False for any item not covered here.
    "can_use(item)": "(_is_magic_item(item) and item and Magic_Meter)
            or (_is_adult_item(item) and is_adult and item)
            or (_is_magic_arrow(item) and is_adult and item and Bow and Magic_Meter)
            or (_is_child_item(item) and is_child and item)
            or (_is_swappable_adult(item) and can_equip_swap_adult and item)
            or (_is_swappable_child(item) and can_equip_swap_child and item)
            or (_adult_slingshot(item) and is_adult and Bow and can_equip_swap_adult)",
    "_is_magic_item(item)": "item == Dins_Fire or item == Farores_Wind or item == Nayrus_Love or item == Lens_of_Truth",
    "_is_adult_item(item)": "item == Bow or item == Megaton_Hammer or item == Iron_Boots or item == Hover_Boots or item == Hookshot or item == Longshot or item == Silver_Gauntlets or item == Golden_Gauntlets or item == Goron_Tunic or item == Zora_Tunic or item == Scarecrow or item == Distant_Scarecrow or item == Mirror_Shield",
    "_is_child_item(item)": "item == Slingshot or item == Boomerang or item == Kokiri_Sword or item == Sticks or item == Deku_Shield",
    "_is_magic_arrow(item)": "item == Fire_Arrows or item == Light_Arrows or item == Blue_Fire_Arrows",
    "_is_swappable_adult(item)": "item == Boomerang or item == Sticks",
    "_is_swappable_child(item)": "item == Megaton_Hammer",
    "_adult_slingshot(item)": "item == Slingshot",

    # Biggoron's trade path
    # ER with certain settings disables timers and prevents items from reverting on save warp.
    # Otherwise, to get to Biggoron requires: a trick, clearing boulders on DMT, or Darunia's Chamber
    "guarantee_trade_path": "disable_trade_revert or plant_beans or can_blast_or_smash or 'Stop GC Rolling Goron as Adult' or (logic_dmt_climb_hovers and can_use(Hover_Boots)) or (logic_biggoron_bolero and not warp_songs and can_play(Bolero_of_Fire) and at('DMC Central Local', can_use(Hookshot) or can_use(Hover_Boots) or can_plant_bean))",
    "guarantee_hint": "(hints == 'mask' and Mask_of_Truth) or (hints == 'agony' and Stone_of_Agony) or (hints != 'mask' and hints != 'agony')",

    "has_fire_source": "can_use(Dins_Fire) or can_use(Fire_Arrows)",
    "has_fire_source_with_torch": "has_fire_source or can_use(Sticks)",

    # Gerudo Fortress
    "can_finish_GerudoFortress": "(gerudo_fortress == 'normal' and
            'Hideout 1 Torch Jail Carpenter' and 'Hideout 2 Torches Jail Carpenter'
            and 'Hideout 3 Torches Jail Carpenter' and 'Hideout 4 Torches Jail Carpenter')
        or (gerudo_fortress == 'fast' and 'Hideout 1 Torch Jail Carpenter')
        or (gerudo_fortress != 'normal' and gerudo_fortress != 'fast')",
    # Mirror shield does not count because it cannot reflect scrub attack.
    "has_shield": "(is_adult and Hylian_Shield) or (is_child and Deku_Shield)",
    # Lets link get into the crouching state
    "can_shield": "(is_adult and (Hylian_Shield or Mirror_Shield)) or (is_child and Deku_Shield)",
    "can_jumpslash": "is_adult or Sticks or Kokiri_Sword or can_use(Megaton_Hammer)",

    # Enemy killing
    "can_kill_armos": "has_soul(Armos) and (is_adult or Sticks or has_explosives or ((Nuts or Boomerang) and (Kokiri_Sword or Slingshot)) or can_use(Megaton_Hammer))",
    "can_kill_octorok": "has_soul(Octorok) and ((is_child and (Boomerang or Slingshot or Deku_Shield)) or (is_adult and (Bow or Hookshot or Hylian_Shield or can_use(Slingshot) or can_use(Boomerang))))",
    "can_kill_octorok_with_bombs": "has_soul(Octorok) and (Bomb_Bag or (is_child and (Boomerang or Slingshot or Deku_Shield)) or (is_adult and (Bow or Hookshot or Hylian_Shield or can_use(Slingshot) or can_use(Boomerang))))",
    "can_kill_octorok_with_bombs_or_magic": "has_soul(Octorok) and (Bomb_Bag or (is_child and (Boomerang or Slingshot or Deku_Shield)) or (is_adult and (Magic_Meter or Bow or Hookshot or Hylian_Shield or can_use(Slingshot) or can_use(Boomerang))))",
    "can_kill_octorok_with_explosives_or_magic": "has_soul(Octorok) and (has_explosives or (is_child and (Boomerang or Slingshot or Deku_Shield)) or (is_adult and (Magic_Meter or Bow or Hookshot or Hylian_Shield or can_use(Slingshot) or can_use(Boomerang))))",
    "can_kill_beamos": "has_soul(Beamos) and has_explosives",
    "can_kill_beamos_with_bomb_flower": "has_soul(Beamos) and (has_explosives or Progressive_Strength_Upgrade)",
    "can_kill_biri": "has_soul(Biri_and_Bari) and ((is_child and ((Kokiri_Sword and (can_take_damage or Nuts or Magic_Meter)) or Sticks or Boomerang or can_use(Megaton_Hammer))) or (is_adult and (can_take_damage or Nuts or Hookshot or Bow or Megaton_Hammer or Magic_Meter or can_use(Sticks))) or Bombs or (Nuts and has_bombchus) or can_use(Dins_Fire))",
    "can_kill_bari": "has_soul(Biri_and_Bari) and ((is_child and ((Kokiri_Sword and (can_take_damage or Nuts or Magic_Meter or Boomerang)) or Sticks or can_use(Megaton_Hammer))) or (is_adult and (can_take_damage or Nuts or Hookshot or Bow or Megaton_Hammer or Magic_Meter or can_use(Sticks))) or Bombs or ((Nuts or can_use(Boomerang)) and has_bombchus) or can_use(Dins_Fire))",
    "can_kill_tailsparan": "has_soul(Tailpasaran) and ((is_child and ((Kokiri_Sword and (Magic_Meter or can_take_damage)) or Sticks or Boomerang or can_use(Megaton_Hammer))) or (is_adult and (Magic_Meter or can_take_damage or Megaton_Hammer or can_use(Sticks) or can_use(Boomerang))))",
    "can_kill_stinger": "has_soul(Stinger) and (is_adult or Kokiri_Sword or Sticks or Slingshot or has_explosives or can_use(Megaton_Hammer))",
    "can_kill_tektite": "has_soul(Tektite) and (is_adult or Kokiri_Sword or Sticks or Slingshot or has_explosives or can_use(Megaton_Hammer))",
    "can_kill_skulltula": "has_soul(Skulltula) and (is_adult or can_child_damage)",
    "can_kill_wolfos": "has_soul(Wolfos) and (is_adult or Slingshot or Sticks or Kokiri_Sword or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_redead": "has_soul(Redead_and_Gibdo) and (is_adult or Sticks or Kokiri_Sword or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_keese": "has_soul(Keese) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Boomerang or (Nuts and has_explosives) or can_use(Megaton_Hammer))",
    "can_kill_keese_stationary": "has_soul(Keese) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Boomerang or has_explosives or can_use(Megaton_Hammer))",
    "can_kill_scrub": "has_soul(Deku_Scrub) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Boomerang or can_use(Dins_Fire) or (Nuts and has_explosives) or can_use(Megaton_Hammer))",
    "can_kill_blue_bubble": "has_soul(Bubble) and (Bombs or (is_adult and (Nuts or Hylian_Shield or Mirror_Shield or Hookshot or Megaton_Hammer or Bow or can_use(Boomerang))) or (is_child and (can_use(Megaton_Hammer) or ( (Boomerang or Nuts or Deku_Shield or Hylian_Shield) and (Sticks or Kokiri_Sword or Slingshot) )) ))",
    "can_kill_green_bubble": "has_soul(Bubble) and (is_adult or Bombs or Sticks or Kokiri_Sword or Slingshot or can_use(Megaton_Hammer))",
    "can_kill_floormaster": "has_soul(Floormaster) and (is_adult or Slingshot or Sticks or Kokiri_Sword or (Bombs and Nuts) or has_bombchus or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_wallmaster": "has_soul(Wallmaster) and (is_adult or can_child_damage)",
    "can_kill_like_like": "has_soul(Like_Like) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Bombs or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_lizalfos": "has_soul(Lizalfos_and_Dinalfos) and (is_adult or Kokiri_Sword or Sticks or Slingshot or (Bombs and Nuts) or can_use(Megaton_Hammer))",
    "can_kill_lizalfos_with_bombs": "has_soul(Lizalfos_and_Dinalfos) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Bombs or can_use(Megaton_Hammer))",
    "can_kill_dinolfos": "has_soul(Lizalfos_and_Dinalfos) and (is_adult or Kokiri_Sword or Sticks or (Slingshot and Nuts) or can_use(Megaton_Hammer))",
    "can_kill_deku_baba": "has_soul(Deku_Baba) and (can_use(Fire_Arrows) or can_use(Ice_Arrows) or can_use(Light_Arrows) or (blue_fire_arrows and can_use(Blue_Fire_Arrows)))",
    "can_kill_withered_deku_baba": "has_soul(Deku_Baba) and (is_adult or Kokiri_Sword or Boomerang)",
    "can_kill_dodongo": "has_soul(Dodongo) and (is_adult or Kokiri_Sword or Sticks or Slingshot or has_explosives or can_use(Megaton_Hammer))",
    "can_kill_dodongo_with_bomb_flower": "has_soul(Dodongo) and (is_adult or Kokiri_Sword or Sticks or Slingshot or has_explosives or Progressive_Strength_Upgrade or can_use(Megaton_Hammer))",
    "can_kill_gohma_larva": "has_soul(Gohma_Larvae) and (is_adult or can_child_attack)",
    "can_kill_baby_dodongo": "has_soul(Baby_Dodongo) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Boomerang or has_explosives or can_use(Megaton_Hammer))",
    "can_kill_stalfos": "has_soul(Stalfos) and (is_adult or Kokiri_Sword or can_use(Megaton_Hammer))",
    "can_kill_peahat": "has_soul(Peahat) and is_child and (Sticks or Kokiri_Sword or Slingshot or has_explosives or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_flying_peahat": "has_soul(Peahat) and is_child and (Slingshot or Bombs or can_use(Dins_Fire))",
    "can_kill_iron_knuckle": "has_soul(Iron_Knuckle) and (is_adult or has_explosives or Kokiri_Sword or (Sticks and Slingshot) or can_use(Megaton_Hammer))",
    "can_kill_guay": "has_soul(Guay) and (is_adult or Kokiri_Sword or Sticks or Slingshot or Boomerang or can_use(Dins_Fire) or can_use(Megaton_Hammer))",
    "can_kill_flare_dancer": "has_soul(Flare_Dancer) and (has_explosives or can_use(Megaton_Hammer) or can_use(Hookshot)) and (is_adult or Sticks or Kokiri_Sword or can_use(Megaton_Hammer))",
    "can_kill_torch_slug": "has_soul(Torch_Slug) and (is_adult or Sticks or Kokiri_Sword or Bombs or can_use(Megaton_Hammer))",
    "can_kill_freezard": "has_soul(Freezard) and (is_adult or Sticks or has_explosives or can_use(Dins_Fire) or can_use(Megaton_Hammer))",

    # Bridge Requirements
    "has_all_stones": "Kokiri_Emerald and Goron_Ruby and Zora_Sapphire",
    "has_all_medallions": "Forest_Medallion and Fire_Medallion and Water_Medallion and Shadow_Medallion and Spirit_Medallion and Light_Medallion",
    "can_open_door_of_time": "(
        (open_door_of_time == 'open') or
        (open_door_of_time == 'sot' and can_play(Song_of_Time)) or
        (open_door_of_time == 'oot_sot' and (Ocarina, 2) and can_play(Song_of_Time)) or
        (open_door_of_time == 'stones' and has_all_stones) or
        (open_door_of_time == 'stones_sot' and has_all_stones and can_play(Song_of_Time)) or
        (open_door_of_time == 'stones_oot_sot' and has_all_stones and (Ocarina, 2) and can_play(Song_of_Time)))",
    "can_build_rainbow_bridge": "(
        (bridge == 'open') or
        (bridge == 'vanilla' and Shadow_Medallion and Spirit_Medallion and Light_Arrows) or
        (bridge == 'stones' and has_stones(bridge_stones)) or
        (bridge == 'medallions' and has_medallions(bridge_medallions)) or
        (bridge == 'dungeons' and has_dungeon_rewards(bridge_rewards)) or
        (bridge == 'tokens' and (Gold_Skulltula_Token, bridge_tokens)) or
        (bridge == 'hearts' and has_hearts(bridge_hearts)))",
    "can_trigger_lacs": "(
        (lacs_condition == 'vanilla' and Shadow_Medallion and Spirit_Medallion) or
        (lacs_condition == 'stones' and has_stones(lacs_stones)) or
        (lacs_condition == 'medallions' and has_medallions(lacs_medallions)) or
        (lacs_condition == 'dungeons' and has_dungeon_rewards(lacs_rewards)) or
        (lacs_condition == 'tokens' and (Gold_Skulltula_Token, lacs_tokens)) or
        (lacs_condition == 'hearts' and has_hearts(lacs_hearts)))",
    "can_receive_ganon_bosskey": "(
        (shuffle_ganon_bosskey == 'stones' and has_stones(ganon_bosskey_stones)) or
        (shuffle_ganon_bosskey == 'medallions' and has_medallions(ganon_bosskey_medallions)) or
        (shuffle_ganon_bosskey == 'dungeons' and has_dungeon_rewards(ganon_bosskey_rewards)) or
        (shuffle_ganon_bosskey == 'tokens' and (Gold_Skulltula_Token, ganon_bosskey_tokens)) or
        (shuffle_ganon_bosskey == 'hearts' and has_hearts(ganon_bosskey_hearts))) or
        (shuffle_ganon_bosskey == 'triforce' and (Triforce_Piece, triforce_goal_per_world)) or
        (shuffle_ganon_bosskey != 'stones' and shuffle_ganon_bosskey != 'medallions' and
        shuffle_ganon_bosskey != 'dungeons' and shuffle_ganon_bosskey != 'tokens' and
        shuffle_ganon_bosskey != 'hearts' and shuffle_ganon_bosskey != 'triforce')",

    # Dungeon Shortcuts
    "deku_tree_shortcuts": "'Deku Tree' in dungeon_shortcuts",
    "dodongos_cavern_shortcuts": "'Dodongos Cavern' in dungeon_shortcuts",
    "jabu_shortcuts": "'Jabu Jabus Belly' in dungeon_shortcuts",
    "forest_temple_shortcuts": "'Forest Temple' in dungeon_shortcuts",
    "fire_temple_shortcuts": "'Fire Temple' in dungeon_shortcuts",
    "shadow_temple_shortcuts": "'Shadow Temple' in dungeon_shortcuts",
    "spirit_temple_shortcuts": "'Spirit Temple' in dungeon_shortcuts",
    "king_dodongo_shortcuts": "region_has_shortcuts('King Dodongo Boss Room')",

    # New Item Groups
    "first_person_item": "(is_adult and (Bow or Fire_Arrows or Ice_Arrows or Blue_Fire_Arrows or Light_Arrows or Hookshot or (
        can_equip_swap_adult and Slingshot))) or
        (is_child and (Slingshot or (can_equip_swap_child and (Bow or Fire_Arrows or Ice_Arrows or Blue_Fire_Arrows or Light_Arrows))))",
    "child_trade_item": "'Weird Egg' or 'Chicken' or 'Zeldas Letter' or 'Keaton Mask' or 'Skull Mask' or 'Spooky Mask'
        or 'Mask of Truth' or 'Goron Mask' or 'Zora Mask' or  'Gerudo Mask' or 'Bunny Hood'",
    "magic_spell": "Dins_Fire or Farores_Wind or Nayrus_Love",

    # Enableable Glitchless Tech
    "can_megajump": "adv_hovers_recoil and can_use(Hover_Boots) and has_explosives and can_live_dmg(0.5,False)",
    "can_damage_boost": "adv_damage_boost and has_both_explosives and can_shield and can_live_dmg(0.5,False)",
    "can_hovers_recoil": "adv_hovers_recoil and can_use(Hover_Boots) and (
        Megaton_Hammer or (has_explosives and can_live_dmg(0.5,False))
        )",

    # Settings shortcuts
    "not_ohko": "damage_multiplier != 'ohko'",

    # Enableable Glitches
    "can_groundjump": "(glitch_groundjump and can_shield and Bombs)
        or (adv_recoil_jump and can_shield and has_explosives)",
    "can_equip_swap_child": "glitch_equip_swap and is_child and (Sticks or Slingshot or Boomerang or has_bottle or child_trade_item
        or magic_spell)",
    "can_equip_swap_adult": "glitch_equip_swap and is_adult and (magic_spell)",


    "can_mega": "glitch_megaflip and has_explosives and can_shield",
    "can_isg": "glitch_isg and can_shield and (is_adult or Sticks or Kokiri_Sword or can_use(Megaton_Hammer))",
    "can_hover": "glitch_hovering and can_isg and has_both_explosives",
    "can_hover_no_trick": "can_shield and can_jumpslash and has_both_explosives", # used for selective cases where specific hovers might be enableable without the global hovering glitch
